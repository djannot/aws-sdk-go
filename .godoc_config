{
	"PkgHandler": {
		"Pattern":          "/sdk-for-go/api/",
		"StripPrefix":     "/sdk-for-go/api",
		"Include":         ["/src/github.com/djannot/aws-sdk-go/aws", "/src/github.com/djannot/aws-sdk-go/service"],
		"Exclude":         ["/src/cmd", "/src/github.com/djannot/aws-sdk-go/awstesting", "/src/github.com/djannot/aws-sdk-go/awsmigrate"],
		"IgnoredSuffixes": ["iface"]
	},
	"Github": {
		"Tag": "master",
		"Repo": "/aws/aws-sdk-go",
		"UseGithub": true
	}
}
